memories_unlocked = Array[ExtResource("5_plb6l")]([SubResource("Resource_25o07")])
startDay = 2

[node name="conv3" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "Who is it, exactly, that knew Constantine?"
patron_response = "I've, uh, said too much. It's not my business, really. "
memories_required = Array[ExtResource("5_plb6l")]([SubResource("Resource_kcntu")])
memories_unlocked = Array[ExtResource("5_plb6l")]([SubResource("Resource_xwm3h")])
startDay = 2

[node name="conv4" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "Please, tell me, Constantine was a friend of mine"
patron_response = "Are you sure? I guess it's alright. It's Trent. He's my neighbor, and he works at the same bank as Constantine. Constantine is his boss."
memories_required = Array[ExtResource("5_plb6l")]([SubResource("Resource_wdawk")])
memories_unlocked = Array[ExtResource("5_plb6l")]([SubResource("Resource_o85on")])
startDay = 2

[node name="conv5" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "Trent hasn't been in today. He usually comes by."
patron_response = "I see. Do you think he'll come later?"
memories_required = Array[ExtResource("5_plb6l")]([SubResource("Resource_jpxtr")])
memories_unlocked = Array[ExtResource("5_plb6l")]([SubResource("Resource_tmwld")])
startDay = 2

[node name="conv6" type="Node" parent="Conversations"]
