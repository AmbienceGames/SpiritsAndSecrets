[node name="conv0" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "Evening, sir. New here?"
patron_response = "What? Oh, yes. I'm Glanthor. Is it true that Constantine was murdered here? Sorry, I mean, it's not my place, but everyone's talking about it."
memories_unlocked = Array[ExtResource("5_plb6l")]([SubResource("Resource_ovenm")])
startDay = 2

[node name="conv1" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "That's none of your business. Constantine was a friend of mine"
patron_response = "Oh, yes, sorry. I understand. Just interested, is all."
memories_required = Array[ExtResource("5_plb6l")]([SubResource("Resource_bkook"), SubResource("Resource_vrrpt")])
startDay = 2

[node name="conv2" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "It's true, yes. Did you know him?"
patron_response = "Never met him. But I know someone who did."
