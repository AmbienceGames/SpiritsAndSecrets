[node name="conv9" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "I'm a good listener, you can talk to me."
patron_response = "*sigh* Fine, I came to keep an eye on him. I know he comes here a lot. It's probably nothing, but I thought... maybe he had something to do with Constantine."
memories_required = Array[ExtResource("5_plb6l")]([SubResource("Resource_jqsiw")])
memories_unlocked = Array[ExtResource("5_plb6l")]([SubResource("Resource_avfw7")])
startDay = 2

[node name="conv10" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "Why would you think that?"
patron_response = "Well, you know he was Trent's boss. Trent really didn't like him. Trent said he was always sitting around while everyone else worked themselves to death for half the money. I'm sorry, I know he was your friend. That's just how Trent talked."
memories_required = Array[ExtResource("5_plb6l")]([SubResource("Resource_swdxc")])
memories_unlocked = Array[ExtResource("5_plb6l")]([SubResource("Resource_s5ey6")])
startDay = 2

[node name="conv11" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "He talked like this often?"
patron_response = "I mean, I didn't talk to him often, but when I did, he was complaining about Constantine. One night, I ran into him while he was drunk. He said he wished Constantine would just go away so he could have the manager position."
memories_required = Array[ExtResource("5_plb6l")]([SubResource("Resource_pyagm"), SubResource("Resource_3nuo2")])
memories_unlocked = Array[ExtResource("5_plb6l")]([SubResource("Resource_t4e6q")])
startDay = 2

