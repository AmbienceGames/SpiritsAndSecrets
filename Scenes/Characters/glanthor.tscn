[node name="conv7" type="Node" parent="Conversations"]
script = ExtResource("4_yw2d6")
player_choice = "Why? Can't you just speak with him at home?"
patron_response = "Oh, no, no. I don't want to speak with him. I just want to... nevermind, I shouldn't have come here."
