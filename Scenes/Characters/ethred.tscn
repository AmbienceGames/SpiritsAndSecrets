[node name="conv1" type="Node" parent="Conversations"]
script = ExtResource("4_aoja3")
player_choice = "What have you been up to?"
patron_response = "The usual. Things have been pretty busy for my group because of the necromancer. You wouldn't believe how many skeletons have been walking around. Still, it's lots of work, so it's lots of pay. "
