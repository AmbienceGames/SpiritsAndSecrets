[node name="conv2" type="Node" parent="Conversations"]
script = ExtResource("4_aoja3")
player_choice = "How's the new party been treating you?"
patron_response = "Well, it's not like the old days, you know? I mean, you people get old fast. Must be annoying only living eighty years."
memories_required = Array[ExtResource("3_xujja")]([SubResource("Resource_hyton")])
startDay = 1
endDay = 1

[node name="conv3" type="Node" parent="Conversations"]
script = ExtResource("4_aoja3")
player_choice = "Necromancer been causing trouble?"
